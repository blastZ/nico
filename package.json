{
  "name": "@blastz/nico",
  "version": "0.11.3",
  "description": "Backend framework build on koa",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blastZ/nico.git"
  },
  "scripts": {
    "tsc": "tsc",
    "tsc-watch": "tsc -w",
