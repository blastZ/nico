    "chalk": "^4.1.0",
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-static": "^5.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@blastz/nico-mongo": "^0.1.0",
    "@hapi/joi": "^17.1.1",
    "@types/jest": "^26.0.8",
    "@types/supertest": "^2.0.10",
